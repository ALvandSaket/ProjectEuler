

def if_prime(i) :
    for x in range (2, i) :
        if i % x == 0 :
            return False
    return True
    
le_list = []

i = 2
while True:
    if if_prime(i) :
        le_list.append(i)
        i += 1
    else:
        i += 1
    
    if len(le_list) == 10001 :
        print(i-1)
        break
