
res = 1000

allofit = []
tamam = False
for x in range(1000):
    for y in range(1000):
        new_to_power = x*x + y*y
        new = new_to_power**(0.5)
        if new + x + y == 1000 and x!=new and y!=new :
            print(f'since {x**2} + {y**2} = {new_to_power} and {x} + {y} = {new} , the answer is {x*y*new}')
            break
