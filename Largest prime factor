number = 600851475143

def find_factor(n) :
    f = 2
    while True :
        if n % f == 0 :
            # print(f"biggest factor of number {n} is {int(n/f)}")
            return int(n/f)
        else :
            f += 1

def if_prime(x):
    while True:
        if find_factor(x) == 1 :
            print (f"the biggest prime factor of number {number} is {x}")
            return False
        else:
            x = find_factor(x)

if_prime(number)
